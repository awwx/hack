(base "http://ycombinator.com/arc/arc3.1.tar//arc3.1/"
  (arc3.1/ac       source "ac.scm"
                   apply  store
                   need   arc3.1/as)

  (arc3.1/brackets source "brackets.scm"
                   apply  store)

  (arc3.1/arc      source "arc.arc"
                   prereq (arc3.1/ac arc3.1/brackets))

  (arc3.1/strings  source "strings.arc"
                   prereq arc3.1/arc
                   provides urlencode)

  (arc3.1/pprint   source "pprint.arc"
                   prereq arc3.1/arc)

  (arc3.1/code     source "code.arc"
                   prereq arc3.1/arc)

  (arc3.1/html     source "html.arc"
                   prereq arc3.1/strings)

  (arc3.1/srv      source "srv.arc"
                   prereq arc3.1/html)
  
  (arc3.1/app      source "app.arc"
                   prereq arc3.1/srv)
  
  (arc3.1/prompt   source "prompt.arc"
                   prereq arc3.1/app)

  ; not sure if news needs prompt or not?

  (arc3.1/news     source "news.arc"
                   prereq arc3.1/app))


(base "git://github.com/awwx/lib.git:"

  (aw/begins-rest0
    source "begins-rest0.arc"
    prereq arc3.1/strings)

  (aw/exit-on-eof2
    source "exit-on-eof2.patch"
    patches arc3.1/ac
    provides control-d-exits)

  (aw/erp1
    source "erp1.arc")

  (aw/xloop0
    source "xloop0.arc")

  (aw/equal-wrt-testing0
    source "equal-wrt-testing0.arc")

  (aw/test0
    source "test0.arc"
    prereq aw/equal-wrt-testing0
    provides aw/testing0)

  (aw/extend0
    source "extend0.arc"
    prereq arc3.1/arc)

  (aw/defarc0
    source "defarc0.patch"
    patches arc3.1/ac)

  (aw/defarc-ac0
    source "defarc-ac0.patch"
    patches arc3.1/ac
    prereq aw/defarc0)

  (aw/scheme0
    source "scheme0.arc"
    prereq (aw/extend0 aw/defarc-ac0))

  (aw/parameter0
    source "parameter0.arc"
    prereq (aw/scheme0 aw/extend0))

  (aw/defvar-patch1
    source "defvar-patch1.patch"
    patches arc3.1/ac)

  (aw/defvar3
    source "defvar3.arc"
    prereq (aw/defvar-patch1 aw/scheme0))

  (aw/dynamic0
    source "dynamic0.arc"
    prereq (aw/defvar3 aw/parameter0))

  (aw/implicit3
    source "implicit3.arc"
    prereq aw/dynamic0)

  (aw/homedir0
    source "homedir0.arc"
    prereq aw/scheme0)

  (aw/ret0
    source "ret0.arc")

  (aw/cwd0
    source "cwd0.arc"
    prereq (aw/scheme0 aw/dynamic0 aw/implicit3))

  (aw/partition0
    source "partition0.arc")

  (aw/alistcall1
    source "alistcall1.patch"
    patches arc3.1/ac
    prereq  aw/defarc0)

  (aw/urlencode2
    source "urlencode2.arc"
    prereq aw/scheme0
    provides urlencode)

  (aw/re3
    source "re3.arc"
    prereq aw/scheme0)

  (aw/redef0
    source "redef0.arc")

  (aw/redef0
    source "redef0.arc")

  (aw/defvar4
    source "defvar4.arc"
    prereq (aw/redef0 aw/defvar-patch1 aw/scheme0)
    replaces aw/defvar3
    recommended)

  (aw/urlencode3
    source "urlencode3.arc"
    prereq aw/scheme0
    provides urlencode)

  (aw/al0
    source "al0.arc")

  (aw/implicitreq0
    source "implicitreq0.arc"
    prereq (arc3.1/srv aw/implicit3 aw/extend0))

  (aw/srv-misc1
    source "srv-misc1.arc"
    prereq (arc3.1/srv aw/scheme0))

  (aw/srv-misc2
    source "srv-misc2.arc"
    prereq (arc3.1/srv aw/scheme0))

  (aw/baseop1
    source "baseop1.arc"
    prereq (aw/extend0 aw/implicitreq0 aw/srv-misc2))

  (aw/match0
    source "match0.arc"
    prereq (aw/implicit3 aw/test0 aw/ret0 aw/xloop0))

  (aw/defrule0
    source "defrule0.arc"
    prereq arc3.1/arc)

  ;; todo don't need redef if not loading arc3.1/strings

  (aw/url0
    source "url0.arc"
    prereq (arc3.1/arc urlencode aw/redef0))

  (aw/readline2
    source "readline2.arc"
    prereq (arc3.1/arc aw/xloop0 aw/redef0))

  (aw/parseurl0
    source "parseurl0.arc"
    prereq (arc3.1/arc arc3.1/strings aw/re3))

  (aw/between0
    source "between0.arc"
    prereq arc3.1/arc)

  (aw/tojson0
    source "tojson0.arc"
    prereq (arc3.1/arc aw/between0)))

; todo I don't know what the right way to handle versions is

(base "git://github.com/awwx/hack.git#v10:"

  (aw/hackinator10/file
    source "file.arc"
    prereq (arc3.1/arc arc3.1/strings))

  ;; todo there's also a dependency on wget

  (aw/hackinator10/download
    source "download.arc"
    prereq (arc3.1/arc aw/begins-rest0 aw/scheme0 aw/homedir0
            aw/ret0 aw/testing0 aw/hackinator10/file))

  (aw/hackinator10/satisfy
    source "satisfy.arc"
    prereq (arc3.1/arc arc3.1/strings aw/implicit3 aw/testing0
            aw/urlencode3 aw/alistcall1))

  ;; todo dependencies on sha224sum, tar, git

  (aw/hackinator10/source
    source "source.arc"
    prereq (arc3.1/arc arc3.1/strings aw/cwd0 aw/hackinator10/download
            aw/testing0 aw/hackinator10/satisfy aw/re3 aw/begins-rest0))

  (aw/hackinator10/rsyntax
    source "rsyntax.arc"
    prereq (arc3.1/arc arc3.1/strings aw/implicit3 aw/hackinator10/satisfy
            aw/hackinator10/file aw/hackinator10/source
            aw/re3 aw/readline2))

  (aw/hackinator10/recipe
    source "recipe.arc"
    prereq (arc3.1/arc aw/partition0 aw/implicit3 aw/hackinator10/rsyntax
            aw/hackinator10/source))

)
